<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tela.tearapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal" >
            <intent-filter>
                <action android:name="android.intent.action.PRINCIPAL" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Cronometria"
            android:label="@string/title_activity_cronometria1" >
        </activity>
        <activity
            android:name=".Cronometria2"
            android:label="@string/title_activity_cronometria2" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Cronometria3"
            android:label="@string/title_activity_cronometria3" >
        </activity>
        <activity
            android:name=".Cronometria4"
            android:label="@string/title_activity_cronometria4" >
        </activity>
        <activity
            android:name=".Configuracoes"
            android:label="@string/title_activity_configuracoes" >
        </activity>
    </application>

</manifest>
